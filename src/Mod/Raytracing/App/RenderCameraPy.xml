    <Attribute Name="Focaldistance" ReadOnly="false">
      <Documentation>
        <UserDocu>Controls the cameras focal distance</UserDocu>
      </Documentation>
      <Parameter Name="FocalDistance" Type="Float"/>
    </Attribute>
      <Attribute Name="Fov" ReadOnly="false">
      <Documentation>
        <UserDocu>Set the Field of View for the Camera (deg)</UserDocu>
      </Documentation>
      <Parameter Name="Fov" Type="Float"/>
    </Attribute>
    <Attribute Name="Autofocus" ReadOnly="false">
      <Documentation>
        <UserDocu>Controls the cameras focal distance</UserDocu>
      </Documentation>
      <Parameter Name="AutoFocus" Type="Boolean"/>
    </Attribute>
  </PythonExport>
